                      {{#if building}}
                        <div class="contact_form_title">Building</div>
                        <form action="{{link}}" method="{{method}}" id="contact_form" enctype="multipart/form-data">
                            <div class="contact_form_inputs d-flex flex-md-row flex-column justify-content-between align-items-between">
                                <input type="text" id="contact_form_name" class="contact_form_name input_field" placeholder="Your name" required="required"  name="name">
                                <input type="text" id="contact_form_address" class="contact_form_address input_field" placeholder="Your Address" required="required"  name="address">
                                <input type="text" id="contact_form_block_no" class="contact_form_block_no input_field" placeholder="Block no" name="block_no">
                            </div>

                            <div class="contact_form_inputs d-flex flex-md-row flex-column justify-content-between align-items-between">
                                <input type="text" id="contact_form_district" class="contact_form_district input_field" placeholder="District" required="required"  name="district">
                                <input type="number" id="contact_form_ward_no" class="contact_form_ward_no input_field" placeholder="Ward no" required="required"  name="ward_no">
                                <input type="text" id="contact_form_street_name" class="contact_form_street_name input_field" placeholder="Street name" name="street">
                            </div>

                            <div class="contact_form_inputs d-flex flex-md-row flex-column justify-content-between align-items-between">
                                <input type="text" id="contact_form_building_licence_no" class="contact_form_building_licence_number input_field" placeholder="Licence number" required="required"  name="licence_no">
