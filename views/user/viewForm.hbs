                                <input type="text" id="contact_form_phone" class="contact_form_phone input_field" placeholder="Your phone number" name="mob_number">
                                <input type="text" id="contact_form_phone" class="contact_form_phone input_field" hidden name="form_type" value="{{type}}">
                            </div>


                            <div class="contact_form_inputs d-flex flex-md-row flex-column justify-content-between align-items-between" style="position: relative;">
                                <label for="">patta</label>
                                <input type="file" id="contact_form_" class="contact_form_ input_field"  required="required" onchange="viewImage(event,'imgViewpatta')"  name="patta">
                                <img style="left: 60px;position: absolute;top: 70px;" src="" alt="" width="200px" height="200px" id="imgViewpatta">

                                <label for="">Reg. document</label>
                                <input type="file" id="contact_form_" class="contact_form_ input_field"  required="required"  name="reg_document" onchange="viewImage(event,'imgViewreg')">
                                <img style="left: 400px;position: absolute;top: 70px;" src="" alt="" width="200px" height="200px" id="imgViewreg">

                                <label for="">Cover letter</label>
                                <input type="file" id="contact_form_" class="contact_form_ input_field"  name="covering_letter" onchange="viewImage(event,'imgViewletter')">
                                <img style="left: 800px;position: absolute;top: 70px;" src="" alt="" width="200px" height="200px" id="imgViewletter">
                            </div>




                            
                            <div class="contact_form_button" style="margin-top: 14rem;">
                                <button type="submit" class="buttonContact contact_submit_button">Submit</button>
                            </div>
                        </form>
