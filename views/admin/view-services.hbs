<section class="mt-5" style="    display: flex;flex-direction: column;justify-content: center;align-items: center;">
    <div class="">
        {{#if response.status}}
        {{#each response.data}}

        <div class="body-div">
            <div class="" style="    display: flex;justify-content: space-between;">
                <a class="body-div-a-tag" style="color: black;text-decoration:none;" href="/admin/viewServices/{{this.birth-certificates}}">{{this.name}}</a> 
                <a class="body-div-a-tag" style="color: blue;" href="/admin/viewServices/{{this.birth-certificates}}" onclick="deleteServices('{{this._id}}','{{this.name}}')">delete</a> 
            </div>
        </div>
        {{/each}}
        {{else}}
        <p>null...</p>
        {{/if}}
